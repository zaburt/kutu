# Ignore bundler config.
/.bundle
/vendor/bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/public/assets
/public/packs
/public/system
/yarn-error.log

/config/database.yml
/config/unicorn.rb
/config/storage.yml
# we may keep this in scm
/config/cable.yml
# Ignore master key for decrypting credentials and more.
/config/master.key
/config/secrets.yml.key

# seeds
/db/base/*.json
/db/seeds/*.json

.byebug_history

# temporary editor files
*.swp
*~
\#*
.#*
.DS_Store
.idea/
.env
.directory
.project
**.orig
dump.rdb

